includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app
        - app-modules
        - bootstrap
        - database
        - routes
        - tests

    level: 5

    excludePaths:
        - storage
        - bootstrap/cache
        - vendor
        - node_modules (?)

    inferPrivatePropertyTypeFromConstructor: true

    scanFiles:
        - bootstrap/app.php
